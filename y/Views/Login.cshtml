@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<ContentModels.Login>
@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels;
@using Microsoft.Extensions.DependencyInjection;

@{
    Layout = null;
}
<!DOCTYPE HTML>
<html>
<head>
    <title>News</title>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
    <link rel="stylesheet" href="assets/css/main.css" />
</head>
<body class="is-preload">
    <div id="content">
        <div class="inner">

            @* render login and register partials from starter kit *@
            @await Html.PartialAsync("partials/registeru")
            @await Html.PartialAsync("partials/loginu")
        </div>
    </div>
    <!-- Sidebar -->
    <div id="sidebar">
        @await Html.GetBlockGridHtmlAsync(Model, "side")
        <!-- Copyright -->
        <ul id="copyright">
            <li>&copy; Untitled.</li>
            <li>Design: <a href="http://html5up.net">HTML5 UP</a></li>
        </ul>
    </div>

    <!-- Scripts -->
    <script src="assets/js/jquery.min.js"></script>
    <script src="assets/js/browser.min.js"></script>
    <script src="assets/js/breakpoints.min.js"></script>
    <script src="assets/js/util.js"></script>
    <script src="assets/js/main.js"></script>

</body>
</html>